            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            FloatLayout:
                id: float_itms
                orientation: 'vertical'
                TwoLineAvatarListItem:
                    opacity: 0
                    id: itm_1
                    pos_hint: {"center_y": 1.5}
                    text: "Annat Frågeformulär"
                    secondary_text: "Svara på frågorna för..."
                    ImageLeftWidget:
                        source: "check_icon.png"
                TwoLineAvatarListItem:
                    opacity: 0
                    id: itm_2
                    pos_hint: {"center_y": 1.5}
                    text: "Tillfredsskalan"
                    secondary_text: "Svara på frågorna för tillfredsskalan"
                    ImageLeftWidget:
                        source: "action_icon.png"
                TwoLineAvatarListItem:
                    opacity: 0
                    id: itm_3
                    pos_hint: {"center_y": 1.5}
                    text: "IDS-100"
                    secondary_text: "Svara på frågorna för IDS-100"
                    on_release:
                        app.root.current = "ids100form"
                    ImageLeftWidget:
                        id: ids_100_icon
                        source: "action_icon.png"


<Ids100Form>:
    name: "ids100form"
    BoxLayout:
        canvas.before:
            Color:
                rgba: 0, 0.3, 0.6, 1
            Rectangle:
                size: self.size
                pos: self.pos
    FloatLayout:
        MDCard:
            id: question_card
            orientation: 'vertical'
            size_hint: [1, 0.3]
            pos_hint: {"center_x": 0.5, "center_y": 0.7}
            BoxLayout:
                MDLabel:
                    id: question_label
                    text: "Du kommer nu få svara på 100 frågor. Du ska värdeskatta hur pass väl påståendet stämmer in. Detta gör du genom att markera det svarsalternativ som bäst stämmer. Klicka på 'Nästa' för att starta."
                    color: (0, 0, 0, 1)
                    halign: "center"
                    pos_hint: {"center_y": 0.5}
                    height: self.texture_size[1]
                    text_size: self.width, None
                    font_size: "20dp"
                    size_hint: 1, None
        MDCard:
            id: ans_card
            orientation: 'vertical'
            size_hint: [1, 0.2]
            pos_hint: {"center_x": 0.5, "center_y": 0.35}
            BoxLayout:
                orientation: 'vertical'
                BoxLayout:
                    size_hint: [1, 0.5]
                    orientation: 'horizontal'
                    CheckBox:
                        id: chk_0
                        text: "0"
                        color: (0, 0, 0, 1)
                        group: "ans"
                        active: False
                        on_active:
                            root.active_check = self.text
                    CheckBox:
                        id: chk_1
                        text: "1"
                        group: "ans"
                        color: (0, 0, 0, 1)
                        active: False
                        on_active:
                            root.active_check = self.text
                    CheckBox:
                        id: chk_2
                        text: "2"
                        group: "ans"
                        color: (0, 0, 0, 1)
                        active: False
                        on_active:
                            root.active_check = self.text
                    CheckBox:
                        id: chk_3
                        text: "3"
                        group: "ans"
                        color: (0, 0, 0, 1)
                        active: False
                        on_active:
                            root.active_check = self.text
                BoxLayout:
                    size_hint: [1, 0.3]
                    orientation: 'horizontal'
                    MDLabel:
                        halign: "center"
                        text: "Aldrig"
                        color: (0, 0, 0, 1)
                    MDLabel:
                        halign: "center"
                        text: "Ibland"
                        color: (0, 0, 0, 1)
                    MDLabel:
                        halign: "center"
                        text: "Ofta"
                        color: (0, 0, 0, 1)
                    MDLabel:
                        halign: "center"
                        text: "Nästan Alltid"
                        color: (0, 0, 0, 1)

        MDRectangleFlatButton:
            pos_hint: {"center_x": 0.5, "center_y": 0.15}
            size_hint: [0.4, 0.08]
            text: "Nästa"
            font_size: "20dp"
            text_color: 0, 0, 1, 1
            md_bg_color: 1, 1, 1, 0.8
            on_release:
                root.next_question()

        MDRectangleFlatButton:
            pos_hint: {"center_x": 0.5, "center_y": 0.05}
            size_hint: [0.4, 0.08]
            text: "Autofyll"
            font_size: "20dp"
            text_color: 0, 0, 1, 1
            md_bg_color: 1, 1, 1, 0.8
            on_release:
                root.autofill_questions()


<Ids100Results>:
    name: "ids100results"
    BoxLayout:
