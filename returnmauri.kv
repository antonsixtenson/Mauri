<UserLogin>:
    name: "userlogin"
    on_enter: root.anim_card(card_user_login)
    BoxLayout:
        canvas.after:
            Color:
                rgba: 0, 0.3, 0.6, 1
            Rectangle:
                size: self.size
                pos: self.pos
    BoxLayout:
        orientation: 'vertical'
        MDToolbar:
            title: "Mauri"
            right_action_items: [["account-supervisor-circle", lambda x: root.goto_superlogin()]]
        BoxLayout:
    FloatLayout:
        MDCard:
            id: card_user_login
            opacity: 0
            orientation: 'vertical'
            size_hint: [0.9, 0.6]
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            BoxLayout:
                orientation: 'vertical'
                size_hint: [1, 0.1]
                MDLabel:
                    halign: "center"
                    text: "Logga in"
                    color: 0, 0, 0, 0.8
                    font_size: "25dp"
                    bold: True
                MDLabel:
                    halign: "center"
                    text: "Användare"
                    color: 0, 0.1, 0.3, 0.8
                    font_size: "15dp"
            BoxLayout:
                size_hint: [0.8, 0.1]
                pos_hint: {"center_x": 0.5}
                MDTextField:
                    id: email
                    hint_text: 'Email'
                    helper_text_mode: "on_error"
                    helper_text: "Email Krävs"
                    pos_hint: {"center_y": 0.5}
                    mode: "rectangle"
                    required: True
            BoxLayout:
                size_hint: [0.8, 0.1]
                pos_hint: {"center_x": 0.5}
                spacing: "20dp"
                MDTextField:
                    id: pwd
                    icon_left: 'key-variant'
                    hint_text: 'Lösenord'
                    mode: "rectangle"
                    helper_text_mode: "on_error"
                    pos_hint: {"center_y": 0.5}
                    helper_text: "Lösenord Krävs"
                    required: True
                    password: True
            BoxLayout:
                size_hint: [0.8, 0.1]
                pos_hint: {"center_x": 0.5}
                MDFillRoundFlatButton:
                    pos_hint: {"center_x": 0.5}
                    size_hint: [0.7, 0.5]
                    text: "Logga in"
                    font_size: "20dp"
                    on_release:
                        root.login()
            BoxLayout:
                size_hint: [0.4, 0.1]
                pos_hint: {"center_x": 0.5}
                MDRectangleFlatButton:
                    size_hint: [0.7, 0.4]
                    pos_hint: {"center_y": 0.5}
                    text: "Registrera"
                    font_size: "16dp"
                    on_release:
                        app.root.current = "registeruser"


