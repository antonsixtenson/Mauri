                    color: (0, 0, 0, 1)
            BoxLayout:
                size_hint: [0.8, 0.1]
                pos_hint: {"center_x": 0.5}
                spacing: "20dp"
                MDTextField:
                    id: age
                    hint_text: 'Ålder'
                    mode: "rectangle"
                    max_text_length: 2
                    input_filter: "int"
                    pos_hint: {"center_y": 0.5}
            BoxLayout:
                size_hint: [0.8, 0.1]
                pos_hint: {"center_x": 0.5}
                spacing: "20dp"
                MDRaisedButton:
