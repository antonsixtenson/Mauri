                MDFillRoundFlatButton:
                    pos_hint: {"center_x": 0.5}
                    size_hint: [0.7, 0.5]
                    text: "Nästa"
                    font_size: "20dp"
                    on_release:
                        root.anim_to_next(card_1_user, card_2_user)
            BoxLayout:
                size_hint: [0.4, 0.1]

