<UserHome>:
    name: "userhome"
    BoxLayout:
        canvas.before:
            Color:
                rgba: 0, 0.3, 0.6, 1
            Rectangle:
                size: self.size
                pos: self.pos
    BoxLayout:
        orientation: 'vertical'
        MDToolbar:
            title: "Mauri"
            left_action_items: [["home", lambda x: root.goto_userhome()]]
            right_action_items: [["logout", lambda x: root.goto_logout()]]
        BoxLayout:
    FloatLayout:
        MDCard:
            id: card_user_home
            orientation: 'vertical'
            size_hint: [1, 0.6]
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            BoxLayout:
                orientation: 'horizontal'
                FloatLayout:
                    MDIconButton:
                        pos_hint: {"center_x": 0.5, "center_y": 0.5}
                        icon: "head-question-outline"
                        user_font_size: "64sp"
                        md_bg_color: app.theme_cls.primary_color
                        on_release:
                            app.root.transition.direction = "left"
                            app.root.current = "userforms"
                FloatLayout:
                    MDIconButton:
                        pos_hint: {"center_x": 0.5, "center_y": 0.5}
                        icon: "chart-line"
                        user_font_size: "64sp"
                        md_bg_color: app.theme_cls.primary_color
            BoxLayout:
                orientation: 'horizontal'
                FloatLayout:
                    MDIconButton:
                        pos_hint: {"center_x": 0.5, "center_y": 0.5}
                        icon: "calendar-month-outline"
                        user_font_size: "64sp"
                        md_bg_color: app.theme_cls.primary_color
                FloatLayout:
                    MDIconButton:
                        pos_hint: {"center_x": 0.5, "center_y": 0.5}
                        icon: "account-outline"
                        user_font_size: "64sp"
                        md_bg_color: app.theme_cls.primary_color
